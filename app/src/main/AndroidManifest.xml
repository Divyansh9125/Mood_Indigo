<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moodindigo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/images2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/images2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mainapp.MainScreen"></activity>
        <activity
            android:name=".IntroSlidesActivity"
            android:theme="@style/AppTheme.OnlyStatusBar" />
        <activity
            android:name=".HomeScreenActivity"
            android:theme="@style/AppTheme.OnlyStatusBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>